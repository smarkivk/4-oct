package ArrayList;

import java.util.ArrayList;

public class example {
    public static void main(String[] args) {
        ArrayList<String> cars = new ArrayList<String>();
        cars.add("Volvo");
        cars.add("BMW");
        cars.add("Ford");
        cars.add("Mazda");
        System.out.println(cars);//normal use of add
        cars.set(0, "Opel");
        System.out.println(cars);//use of set to modify an element 
    }
}
